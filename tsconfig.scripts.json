{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "outDir": "dist-scripts",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "noEmit": false,
    "resolveJsonModule": true
  },
  "include": ["scripts/**/*.ts"]
}
